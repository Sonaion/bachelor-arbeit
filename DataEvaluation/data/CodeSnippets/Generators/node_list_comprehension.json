        "[AOI=Class]class Node:",
        "    def __init__(self, value):",
        "        self.value = value",
        "        self.left = None",
        "        self.right = None",
        "",
        "    def pre_order_iter(self):",
        "        result = [self]",
        "        if self.left is not None:",
        "            left = self.left.pre_order_iter()",
        "            result = result + left",
        "        if self.right is not None:",
        "            right = self.right.pre_order_iter()",
        "            result = result + right",
        "        return result[/AOI]",
        "",
        "",
        "[AOI=Method][AOI=MethodDeclaration]def function(node):[/AOI]",
        "[AOI=MethodBody]    multiplies = [AOI=ListComprehension][x.value * y.value for x in node.pre_order_iter() for y in node.pre_order_iter()][/AOI]",
        "    total = 0",
        "    scanned = [AOI=ListComprehension][total := total + x for x in multiplies][/AOI]",
        "    return scanned[-1][/AOI][/AOI]",
        "",
        "",
        "[AOI=InputPreparation]n = Node(2)",
        "n.left = Node(1)",
        "n.right = Node(3)[/AOI]",
        "[AOI=StartOfAlgorithm]print(function(n))[/AOI]",
        ""
    ]
}